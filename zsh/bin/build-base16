#!/usr/bin/env zsh

for f in ~/.vim/plugged/base16-vim/colors/base16-*.vim; do
  eval ${$(grep 'let s:gui0. *= ' $f | sed -e 's/let s://' -e 's/ //g'):l}
  print -l \
      "fg              $gui05" \
      "bg              $gui00" \
      "black           $gui00" \
      "red             $gui08" \
      "green           $gui0b" \
      "yellow          $gui0a" \
      "blue            $gui0d" \
      "magenta         $gui0e" \
      "cyan            $gui0c" \
      "white           $gui05" \
      "bright_black    $gui03" \
      "bright_red      $gui08" \
      "bright_green    $gui0b" \
      "bright_yellow   $gui0a" \
      "bright_blue     $gui0d" \
      "bright_magenta  $gui0e" \
      "bright_cyan     $gui0c" \
      "bright_white    $gui07" \
      "comment_fg      $gui03" \
      "cursorline_bg   $gui01" \
      "afg             $gui01" \
      "afg2            $gui01" \
      "afgi            $gui00" \
      "abg             $gui0d" \
      "abg2            $gui0b" \
      "abgi            $gui02" \
      "bfg             $gui05" \
      "bfg2            $gui05" \
      "bfgi            $gui00" \
      "bbg             $gui02" \
      "bbg2            $gui02" \
      "bbgi            $gui01" \
      "cfg             $gui05" \
      "cfg2            $gui05" \
      "cfgi            $gui00" \
      "cbg             $gui01" \
      "cbg2            $gui01" \
      "cbgi            $gui01" \
      "efg             $gui00" \
      "ebg             $gui08" > ~/.zsh/themes/$f:t:r
done
